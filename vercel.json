{
    "version": 2,
    "name": "my-fastapi-app",
    "builds": [
        {
            "src": "main.py",
            "use": "@vercel/python",
            "config": {
                "maxLambdaSize": "50mb"
            }
        }
    ],
    "routes": [
        {
            "src": "/.*",
            "dest": "main.py"
        }
    ]
}